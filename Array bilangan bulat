public class Array {
    public static void main(String[] args) {
    
        int[] angka = new int[5];

        
        angka[0] = 10;
        angka[1] = 20;
        angka[2] = 30;
        angka[3] = 40;
        angka[4] = 50;

        
        System.out.println("Nilai pada indeks 0: " + angka[0]);
        System.out.println("Nilai pada indeks 2: " + angka[2]);

        
        int panjangArray = angka.length;
        System.out.println("Panjang array: " + panjangArray);
    }
}
