package isc;

public class latihan {

    public static void main(String[] args) {
        int nilai1 = 7;
        int nilai2 = 13;

        if (nilai1 > 5 || nilai2 < 10) {
            System.out.println("Salah satu kondisi terpenuhi");
        } else {
            System.out.println("Kedua kondisi tidak terpenuhi");
        }
    }

}
