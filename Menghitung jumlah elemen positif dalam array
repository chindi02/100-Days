public class Main {
    public static void main(String[] args) {
        int[] array = {-1, 2, -3, 4, 5, -6, 7};
        int jumlahPositif = 0;
        for (int num : array) {
            if (num >= 0) {
                jumlahPositif += num;
            }
        }
        System.out.println("Jumlah derajat positif dalam array: " + jumlahPositif);
    }
}
